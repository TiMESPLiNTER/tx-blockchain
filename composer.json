{
    "name": "timesplinter/tx-blockchain",
    "license": "MIT",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs", "no-api": true, "url": "git@github.com:TiMESPLiNTER/blockchain.git"
        }
    ],
    "require": {
        "timesplinter/blockchain": "dev-tx",
        "ionux/phactor": "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Timesplinter\\TxBlockchain\\": "src/"
        }
    },
    "extra": {
        "patches": {
            "ionux/phactor": {
                "PHP 7.2": "https://patch-diff.githubusercontent.com/raw/ionux/phactor/pull/8.patch",
                "Fix parseCompressedPublicKey": "https://patch-diff.githubusercontent.com/raw/ionux/phactor/pull/10.patch"
            }
        }
    }
}